@model IEnumerable<BackEnd.Entities.Matricula>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaMatricula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCursoFkNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstudianteFkNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPlanEstudioFkNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaMatricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCursoFkNavigation.CodigoCurso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstudianteFkNavigation.Genero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPlanEstudioFkNavigation.NombrePlanEstduio)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMatricula">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMatricula">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMatricula">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
